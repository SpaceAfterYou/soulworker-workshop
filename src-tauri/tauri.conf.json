{
  "package": { "productName": "SoulWorker Workshop", "version": "25.0.0" },
  "build": { "distDir": "../dist", "devPath": "http://localhost:5173", "beforeDevCommand": "pnpm vite:dev", "beforeBuildCommand": "pnpm vite:build" },
